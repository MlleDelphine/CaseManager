{% extends 'base.html.twig' %}

{% block body %}
    <h1>Corporationjobstatus</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ corporationJobStatus.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ corporationJobStatus.name }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ corporationJobStatus.slug }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{% if corporationJobStatus.created %}{{ corporationJobStatus.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{% if corporationJobStatus.updated %}{{ corporationJobStatus.updated|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('corporation-job-status_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('corporation-job-status_edit', { 'id': corporationJobStatus.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
