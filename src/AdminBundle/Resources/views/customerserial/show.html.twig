{% extends 'base.html.twig' %}

{% block body %}
    <h1>Customerserial</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ customerSerial.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ customerSerial.name }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ customerSerial.slug }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{% if customerSerial.created %}{{ customerSerial.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{% if customerSerial.updated %}{{ customerSerial.updated|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('customer_serial_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('customer_serial_edit', { 'id': customerSerial.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
