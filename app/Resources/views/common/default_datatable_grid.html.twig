{% if childrenRow is not defined %}
    {% set childrenRow = false %}
    {% set childrenProperties = [] %}
    {% set childrenRouteName = "" %}
{% endif %}

{{ grid(grid, ':common:common_block_js_datatable.jquery.twig',  "", {'childrenRow': childrenRow, 'childrenProperties': childrenProperties, 'childrenRouteName': childrenRouteName }) }}

<script type="text/javascript">
    $(function () {
        {% if deleteModals is defined %}
        {% for index, params in deleteModals %}
        console.log("print deleteModals");
        {% include ":common:delete_pop_up.js.twig" with params %}
        {% endfor %}
        {% else %}
        console.log("no deleteModals to print ...");
        {% endif %}
    });
</script>